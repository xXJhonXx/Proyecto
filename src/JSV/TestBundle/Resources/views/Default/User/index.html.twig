<h2>Users</h2>
<table border="1">
<tr>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Role</th>
    <th>Created</th>
    <th>Updated</th>
    <th>Actions</th>
</tr>      
    {% for user in users %}
        <tr>
            <td>{{ user.username }}</td>
            <td>{{ user.firsname }}</td>
            <td>{{ user.lastname }}</td>
            <td>{{ user.email }}</td>
            <td>{{ user.role }}</td>
            <td>{{ user.createdAt!date('d-m-y H:i') }}</td>
            <td>{{ user.updatedAt!date('d-m-y H:i') }}</td>
            <td>
                <a href="">View</a>
                <a href="">Edit</a>
                <a href="">Delete</a>
            </td>
        </tr>
    {% endfor %}

</table>